{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jasmine": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
        "jsx-a11y/anchor-is-valid": [ "error", {
            "components": [ "Link" ],
            "specialLink": [ "hrefLeft", "hrefRight" ],
            "aspects": ["invalidHref", "preferButton" ]
          }]
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "flowtype"
    ]
}